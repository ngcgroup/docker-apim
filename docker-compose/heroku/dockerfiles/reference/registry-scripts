api_admin_server_url=https://apistudio.bhn.technology server=apistudio.bhn.technology XML=$(cat auth.xml.template | sed "s/ADMIN_USER/${ADMIN_USER}/g" | sed "s/ADMIN_PASSWORD/${ADMIN_PASSWORD}/g" | sed "s/SERVER/${server}/g")
COOKIE=$(curl -k -s -o /dev/null -D -  -H 'SOAPAction: urn:login' -H 'Content-Type: text/xml' -d "$XML" ${api_admin_server_url}/services/AuthenticationAdmin | grep set-cookie | cut -d':' -f2 | cut -d ';' -f1 | sed 's/^ //g')

